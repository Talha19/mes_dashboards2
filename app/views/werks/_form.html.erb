<div id="wrapper">
	<div id="page-wrapper">
		<div class="row">
			<div class="col-lg-12">
				<%= bootstrap_form_for @werk do |f| %>
				<%= f.text_field :werk %>
				<%= f.text_field :stadt %>
				<%= f.text_field :land %>
				<%= f.file_field :avatar%>

				<%= f.submit "Werk anlegen" , class: 'btn btn-success'%>
				<% end %>
			</div>
		</div>
	</div>
</div>